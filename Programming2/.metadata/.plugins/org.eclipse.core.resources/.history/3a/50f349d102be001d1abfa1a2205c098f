public class bottomRight {

	public static void main(String[] args) {
		

		bottomRight(6);
		
	}
	
	
	public static void bottomRight(int num) {
		
		for(int i = 0; i < num; i++) {
			
			
			
			for(int b = 1; b < num; x++) {
				System.out.print("*");
			}
			
	
			for(int x = 1; x < num; x++) {
				System.out.print(" ");
				
				
			}
			
			
			System.out.println("*");
			
				
}
	
		
		
	}
	
	

}





